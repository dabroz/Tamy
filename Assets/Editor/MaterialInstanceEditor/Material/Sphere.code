	VSOutput Out = (VSOutput)0;

	float4 Output_006 = mul( In.vPosition, Transform_004 );
	float4 Output_007 = mul( In.vPosition, Transform_005 );
	float3 Output_008 = mul( In.vNormal, (float3x3)Transform_005 ) ;
